{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "src/",
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "Node",
    "noImplicitAny": false,
    "sourceMap": true,
    "strict": false,
    "target": "ESNext"
  },
  "include": [
    "src/**/*."
  ],
  "exclude": [
    "node_modules"
  ]
}
